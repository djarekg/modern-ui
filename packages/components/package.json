{
  "name": "@mui/components",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "packageManager": "bun@1.1.41",
  "author": "Dustin Griffith",
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/modern-ui"
  },
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./theme/theme.css": "./dist/theme/theme.css",
    "./button/button.js": {
      "types": "./dist/button/button.d.ts",
      "import": "./dist/button/button.js"
    },
    "./button/flat-button.js": {
      "types": "./dist/button/flat-button.d.ts",
      "import": "./dist/button/flat-button.js"
    },
    "./button/outline-button.js": {
      "types": "./dist/button/outline-button.d.ts",
      "import": "./dist/button/outline-button.js"
    },
    "./dialog/dialog.js": {
      "types": "./dist/dialog/dialog.d.ts",
      "import": "./dist/dialog/dialog.js"
    },
    "./dialog/confirm.js": {
      "types": "./dist/dialog/confirm.d.ts",
      "import": "./dist/dialog/confirm.js"
    },
    "./icon/icon.js": {
      "types": "./dist/icon/icon.d.ts",
      "import": "./dist/icon/icon.js"
    },
    "./table/*.js": {
      "types": "./dist/table/*.d.ts",
      "import": "./dist/table/*.js"
    },
    "./text-field/text-field.js": {
      "types": "./dist/text-field/text-field.d.ts",
      "import": "./dist/text-field/text-field.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "task build",
    "build": "task build"
  },
  "dependencies": {
    "@lit-labs/signals": "^0.1.1",
    "@mui/core": "workspace:*",
    "@mui/web": "workspace:*",
    "@open-wc/dedupe-mixin": "^1.4.0",
    "signal-utils": "^0.21.1"
  },
  "peerDependencies": {
    "lit": "^3.2.1",
    "typescript": "^5"
  }
}
