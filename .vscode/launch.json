{
  "configurations": [
    {
      "name": "Launch demo:serve",
      "cwd": "${workspaceFolder}/apps/demo",
      "request": "launch",
      "type": "node",
      "runtimeExecutable": "bun",
      "runtimeArgs": ["dev"],
      "skipFiles": ["**/node_internals/**", "!**/node_modules/**/@mu/**"],
      "console": "integratedTerminal"
    },
    {
      "name": "Launch demo:app",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:8000/apps/demo",
      "webRoot": "${workspaceFolder}/apps/demo",
      "userDataDir": "${workspaceFolder}/.out/edge/apps"
    }
  ],
  "compounds": [
    {
      "name": "Launch demo",
      "configurations": ["Launch demo:serve", "Launch demo:app"]
    }
  ]
}
