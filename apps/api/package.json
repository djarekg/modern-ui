{
  "name": "@mui/api",
  "version": "1.0.0",
  "private": true,
  "author": "Dustin Griffith",
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/modern-ui"
  },
  "license": "MIT",
  "packageManager": "bun@1.1.41",
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "db:generate": "rm -rf prisma/dev.db && bunx prisma generate && bunx prisma db push && bun prisma/seed.ts",
    "dev": "bun src/index.ts",
    "dev:debug": "bun --inspect=localhost:4004 --watch src/index.ts",
    "build": "task build"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@prisma/client": "^6.2.1",
    "elysia": "^1.2.10",
    "haunted": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@types/bun": "^1.1.16",
    "prisma": "^6.2.1",
    "typescript": "^5"
  },
  "trustedDependencies": ["@prisma/client", "@prisma/engines", "prisma"]
}
