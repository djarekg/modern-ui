{
  "name": "@mui/demo",
  "version": "1.0.0",
  "private": true,
  "author": "Dustin Griffith",
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/modern-ui"
  },
  "license": "MIT",
  "packageManager": "bun",
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./": {
      "types": "./dist/**/*.d.ts",
      "import": "./dist/**/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "bunx vite",
    "build": "task build && bunx vite build",
    "preview": "bunx vite preview",
    "generate": "bunx apollo service:download --endpoint=http://localhost:4003/graphql graphql-schema.json"
  },
  "dependencies": {
    "@apollo/client": "^3.13.4",
    "@elysiajs/eden": "^1.2.0",
    "@lit-labs/router": "^0.1.4",
    "@lit-labs/signals": "^0.1.2",
    "@lit/context": "^1.1.4",
    "@mui/components": "workspace:*",
    "@mui/core": "workspace:*",
    "@mui/graphql": "workspace:*",
    "graphql": "^16.10.0",
    "haunted": "^6.1.0",
    "lit": "^3.2.1",
    "signal-utils": "^0.21.1"
  },
  "devDependencies": {
    "@apollo/client-devtools-vscode": "^4.19.11",
    "@types/bun": "^1.2.2",
    "@types/graphql": "^14.5.0",
    "apollo": "^2.34.0",
    "elysia": "^1.2.14",
    "typescript": "^5.7.3",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.1.0"
  },
  "trustedDependencies": ["esbuild"]
}
