{
  "name": "@mui/demo",
  "version": "1.0.0",
  "private": true,
  "author": "Dustin Griffith",
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/modern-ui"
  },
  "license": "MIT",
  "packageManager": "bun",
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./": {
      "types": "./dist/**/*.d.ts",
      "import": "./dist/**/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "bunx vite",
    "build": "task build && bunx vite build",
    "preview": "bunx vite preview",
    "generate": "bunx graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.4",
    "@lit-labs/router": "^0.1.4",
    "@lit-labs/signals": "^0.1.2",
    "@lit/context": "^1.1.4",
    "@mui/components": "workspace:*",
    "@mui/core": "workspace:*",
    "@mui/graphql": "workspace:*",
    "graphql": "^16.10.0",
    "haunted": "^6.1.0",
    "lit": "^3.2.1",
    "signal-utils": "^0.21.1"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@apollo/client-devtools-vscode": "^4.19.11",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typed-document-node": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.1",
    "@types/bun": "^1.2.5",
    "@types/graphql": "^14.5.0",
    "elysia": "^1.2.25",
    "typescript": "^5.8.2",
    "unplugin-fonts": "^1.3.1",
    "vite": "^6.2.2"
  },
  "trustedDependencies": ["esbuild"]
}
